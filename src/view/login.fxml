<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefWidth="250.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="photoalbum.controller.Login">
   <children>
      <Label alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="16.0" prefWidth="64.0" text="Welcome to Your Photo Album!" />
      <HBox alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefWidth="200.0">
         <children>
            <Label text="Login:" />
            <TextField fx:id="LoginField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefWidth="200.0">
         <children>
            <Button fx:id="LoginLoginButton" mnemonicParsing="false" onMouseClicked="#LoginClicked" text="Login" />
            <Button fx:id="LoginCloseButton" mnemonicParsing="false" onMouseClicked="#CloseClicked" text="Close" />
         </children>
      </HBox>
   </children>
</VBox>
